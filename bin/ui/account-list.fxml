<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <HBox nodeOrientation="RIGHT_TO_LEFT" prefHeight="82.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="btnLogOut" mnemonicParsing="false" onAction="#logOut" prefWidth="80.0" text="Log Out">
               <font>
                  <Font size="13.0" />
               </font>
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Button>
            <ImageView fx:id="imvImage" fitHeight="38.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </ImageView>
            <Label fx:id="lblUsername" text="Label">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Label>
            <Label prefHeight="27.0" prefWidth="149.0" text="User account list">
               <HBox.margin>
                  <Insets left="50.0" top="50.0" />
               </HBox.margin>
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
               <padding>
                  <Insets bottom="10.0" />
               </padding>
            </Label>
         </children>
      </HBox>
   </top>
   <center>
      <TableView fx:id="tvUsers" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="tcUsername" prefWidth="120.0" text="Username" />
          <TableColumn fx:id="tcGender" prefWidth="120.0" text="Gender" />
            <TableColumn fx:id="tcCareer" prefWidth="120.0" text="Career" />
            <TableColumn fx:id="tcBirthday" prefWidth="120.0" text="Birthday" />
            <TableColumn fx:id="tcBrowser" prefWidth="120.0" text="Browser" />
        </columns>
      </TableView>
   </center>
</BorderPane>
